{
    "name": "puli/web-resource-plugin",
    "description": "A Puli plugin for managing web resources.",
    "homepage": "http://puli.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bschussek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "puli/repository": "1.0.*",
        "puli/repository-manager": "~1.0"
    },
    "require-dev": {
        "puli/cli": "~1.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Puli\\WebResourcePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puli\\WebResourcePlugin\\Tests\\": "tests/",
            "Puli\\RepositoryManager\\Tests\\": "vendor/puli/repository-manager/tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "support": {
        "issues": "https://github.com/puli/issues/issues"
    }
}
